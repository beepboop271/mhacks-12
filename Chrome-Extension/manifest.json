{
  "name": "MHacks2019",
  "version": "1.0",
  "description": "Help me",
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "a.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Help Hackermans"
  }
}
